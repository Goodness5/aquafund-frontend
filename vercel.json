{
  "version": 2,
  "builds": [
    {
      "src": "packages/nextjs/next.config.ts",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "packages/nextjs/$1" }
  ],
  "build": {
    "env": {
      "YARN_ENABLE_IMMUTABLE_INSTALLS": "false",
      "ENABLE_EXPERIMENTAL_COREPACK": "1",
      "VERCEL_TELEMETRY_DISABLED": "1"
    }
  }
}

